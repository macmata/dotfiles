if [ -f ~/.rbenv ]; then
	eval "$(rbenv init -)"
	export PATH="$HOME/.rbenv/bin:$PATH"
	export PATH="$HOME/.rbenv/plugins/ruby-build/bin:$PATH"
fi

if [ -f /usr/bin/vimx ]; then
	export EDITOR=/usr/bin/vimx
	export USE_EDITOR=$EDITOR
	export VISUAL=$EDITOR
fi

[ -f ~/.gtestrc ]  && source ~/.gtestrc

